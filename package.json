{
	"name": "chneau-extension-pack",
	"displayName": "chneau-extension-pack",
	"description": "Chneau's extension pack",
	"version": "0.0.16",
	"publisher": "chneau",
	"license": "MIT",
	"galleryBanner": {
		"color": "#1E1E1E",
		"theme": "dark"
	},
	"keywords": [
		"chneau",
		"auto-setup",
		"personal",
		"settings"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/chneau/vscode-extension-pack"
	},
	"engines": {
		"vscode": "^1.99.0"
	},
	"categories": [
		"Extension Packs"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/main.js",
	"browser": "./out/main.js",
	"scripts": {
		"build": "esbuild ./src/main.ts --outfile=out/main.js --bundle --external:vscode --platform=node --minify",
		"upgrade": "bun update --latest; bun install --dev @types/vscode@1.99.0",
		"publish": "bun run build && vsce package && vsce publish",
		"all": "bun run upgrade; bun run check; bun run lint",
		"check": "rm -rf out; deno fmt --use-tabs --quiet; oxlint --fix-dangerously --quiet; timeout 3s biome check --write --unsafe .",
		"lint": "tsc --noEmit"
	},
	"devDependencies": {
		"@types/node": "^24.10.1",
		"@types/vscode": "1.99.0",
		"@vscode/vsce": "^3.7.0",
		"esbuild": "^0.27.0",
		"typescript": "^5.9.3"
	},
	"extensionPack": [
		"biomejs.biome",
		"chneau.resource-monitor",
		"chneau.saturated-dark-modern",
		"dakara.transformer",
		"esbenp.prettier-vscode",
		"GitHub.copilot-chat",
		"golang.go",
		"grapecity.gc-excelviewer",
		"jebbs.plantuml",
		"ms-dotnettools.csharp",
		"ms-python.python",
		"ms-toolsai.jupyter",
		"ms-vscode.vscode-typescript-next",
		"oven.bun-vscode",
		"Prisma.prisma",
		"yoavbls.pretty-ts-errors"
	]
}
